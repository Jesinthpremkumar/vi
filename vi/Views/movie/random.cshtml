
@model vi.viewModel.RandomMovieViewModel
@{
    ViewBag.Title = "random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.movie.name</h2>

<ul>
    @if (Model.movie.name.Equals("shrek"))
    {
        foreach(var i in Model.customers)
        {
            <li>
                @i.id
                </li>
        }
    }
    </ul>